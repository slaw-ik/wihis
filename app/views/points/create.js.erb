clearOverlays();
Gmaps.map.addMarkers(<%= raw @json %>);
Gmaps.map.callback();
Gmaps.map.map.setCenter(new google.maps.LatLng(<%= @center[:latitude]%>, <%=@center[:longitude]%>));
Gmaps.map.markers[Gmaps.map.markers.length - 1].infowindow.open(Gmaps.map.map, Gmaps.map.markers[Gmaps.map.markers.length - 1].serviceObject);
apply_tagging();